{
  "name": "client",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "scripts": {
    "build": "npm run clean && babel src --out-dir dist",
    "clean": "node build.js",
    "lint": "eslint -c .eslintrc src",
    "reinstall": "npm cache clean --force && npm run clean && rm -rf node_modules/ package-lock.json && npm install",
    "sonar": "node sonar-project.js",
    "start": "cross-env NODE_ENV=production node bin/www",
    "start:dev": "cross-env NODE_ENV=development concurrently -k -n \"Babel,Node\" -p \"[{name}]\" -c \"yellow,green\" \"babel src --out-dir dist --watch\" \"nodemon bin/www\"",
    "webpack": "cross-env NODE_ENV=production webpack --progress",
    "webpack:dev": "cross-env NODE_ENV=development webpack --progress",
    "webpack:watch": "cross-env NODE_ENV=development webpack --watch --progress"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": {
